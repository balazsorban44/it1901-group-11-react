{
  "name": "it1901-group-11",
  "version": "1.0.0-rc.1",
  "private": true,
  "dependencies": {
    "datejs": "^1.0.0-rc3",
    "firebase": "^4.6.1",
    "material-ui": "^0.19.4",
    "material-ui-datetimepicker": "^1.0.2",
    "moment": "^2.19.1",
    "moment-range": "^3.0.3",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-masonry-css": "^1.0.10",
    "react-scripts": "^1.0.16",
    "react-swipeable-views": "^0.12.8",
    "sass-lint": "^1.12.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/sass src/sass/main.sass src/main.css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/sass src/sass/main.sass src/main.css -w -r",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
